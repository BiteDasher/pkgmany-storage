debname=ascii-patrol
debver=1.3
debrel=2
debarch="amd64"
year=2020
debdesc='Terminal Moon Control clone.'
url='http://ascii-patrol.com/'
license="GPL"
debdeps="pulseaudio"
maintainer="Artemii Sudakov <finziyr@yandex.ru>"
